{
  "name": "ellalle-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Privacy-focused trading platform with zero-knowledge technology on Hedera",
  "homepage": "https://ellalle.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnnyduo/stealthflow-aiview.git"
  },
  "keywords": ["hedera", "trading", "defi", "privacy", "zero-knowledge", "web3"],
  "author": "ELLALLE Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --port 4173",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "analyze": "yarn build && npx vite-bundle-analyzer dist",
    "security:audit": "yarn audit --level moderate",
    "deps:update": "yarn upgrade-interactive --latest",
    "deps:check": "yarn outdated",
    "hedera:add-network": "echo 'Add Hedera Testnet to MetaMask manually or the app will prompt'",
    "postinstall": "yarn type-check"
  },
